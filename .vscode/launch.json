{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI: Uvicorn",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--host",
                "localhost",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": false
        },
        {
            "name": "FastAPI: Gunicorn",
            "type": "python",
            "request": "launch",
            "module": "gunicorn",
            "args": [
                "app.main:app",
                "-c",
                "gunicorn.conf.py"
            ],
            "jinja": true,
            "justMyCode": false
        },
        {
            "name": "Python: Debug lab-1",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/lab-1.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Celery: Worker",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "worker.celery",
                "worker",
                "--loglevel=info",
                "--concurrency=1",
                "-E"
            ],
            "jinja": true,
            "justMyCode": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Celery: Beat",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "worker.celery beat",
                "worker",
                "--loglevel=info",
                "--concurrency=1",
                "-E"
            ],
            "jinja": true,
            "justMyCode": false,
            "console": "integratedTerminal"
        }
    ]
}
